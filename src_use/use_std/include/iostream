#include <ios>
#include <streambuf>
#include <istream>
#include <ostream>
 
namespace std {
 
    extern istream cin;
    extern ostream cout;
    extern ostream cerr;
    extern ostream clog;
 
    extern  wistream wcin;
    extern  wostream wcout;
    extern  wostream wcerr;
    extern  wostream wclog;
 
}